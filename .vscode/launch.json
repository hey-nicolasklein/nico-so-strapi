{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Strapi: Develop",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/@strapi/strapi/bin/strapi.js",
      "args": ["develop"],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "env": {
        "NODE_ENV": "development"
      },
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "name": "Strapi: Start",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/@strapi/strapi/bin/strapi.js",
      "args": ["start"],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "env": {
        "NODE_ENV": "production"
      },
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "name": "Strapi: Build",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/@strapi/strapi/bin/strapi.js",
      "args": ["build"],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "env": {
        "NODE_ENV": "production"
      },
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "name": "Seed Example Data",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/seed.js",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "env": {
        "NODE_ENV": "development"
      },
      "skipFiles": [
        "<node_internals>/**"
      ]
    }
  ]
}
